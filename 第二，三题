#include <stdio.h>
int main()
{
	int day = 0;
	int amount = 0;
	int day1 = 0;
	int amount1 = 0;
	scanf_s("%d", &amount);
	amount1 = amount;

	while (amount > 3)
	{
		amount -= 2;
		day++;
	}	
	printf("%d\n", day+1);
	if (amount1 == 0)printf("0");
	if (amount1 <= 2)printf("1");
	if (amount1 > 2)printf("%lld", amount1 / 2);
}
