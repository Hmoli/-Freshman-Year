#include <stdio.h>

int Fibonacci(int n)
{
	int fibonacci[1000] = { 1,1 };
	int i = 0;
	int fi = 0;
	if (n < 3) return 1;
	else
	{
		for (i=2; i < n; i++)
		{
			fibonacci[i] = fibonacci[i - 1] + fibonacci[i - 2];
		}
		return (fibonacci[n-1]);
	}
}
int main()
{
	int n = 0;
	int f = 0;
	scanf_s("%d", &n);
	if (n < 1)printf("数据错误");
	else
	{
		f = Fibonacci(n);
	}
	printf("%d", f);
}
