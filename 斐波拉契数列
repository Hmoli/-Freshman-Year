#include <stdio.h>
#include <time.h>

int count = 0;//用于计数

int Fibonacci2(int m)
{
	if (3 == m) count++;//统计第三个斐波拉契数的出现次数

	if (m < 3)
		return 1;
	else
		return( Fibonacci2(m - 1) + Fibonacci2(m - 2));
}

int Fibonacci1(int n)
{

	int fibonacci[1000] = { 1,1 };
	int i = 0;
	int fi = 0;
	if (n < 3) return 1;
	else
	{
		for (i=2; i < n; i++)
		{
			fibonacci[i] = fibonacci[i - 1] + fibonacci[i - 2];
		}
		return (fibonacci[n-1]);
	}
}
int main()
{
	int n = 0;
	int f = 0;
	int F = 0;
	scanf_s("%d", &n);
	if (n < 1)printf("数据错误");
	else
	{
		f = Fibonacci1(n);
		F = Fibonacci2(n);
	}
	printf("f=%d\n", f);
	printf("F=%d\n", F);
	printf("count=%d\n", count);
}
