#include <stdio.h>

int binsearch(int aim, int v[], int n)
{
	int i = 0;
	for(;v[i]<=aim && i<n ;i++)
	{
		if (v[i] == aim)return (i+1);
	}
	printf("未找到目标数");
	return (aim-1);
}

int main()
{
	int arr[10000] = { 0 };
	int i;
	int n = 0;
	int aim = 0;//要找的数
	int t_aim = 0;//用于存放找到的目标数
	for(i=0; scanf_s("%d",&arr[i])!=EOF;i++)
	{
		;
	}
	printf("输入完成\n");
	scanf_s("%d", &n);
	scanf_s("%d", &aim);
	t_aim = binsearch(aim, arr, n);
	if(t_aim==aim) printf("%d",aim);//检验找到的是否是要找的数
}
