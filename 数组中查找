#include <stdio.h>

binsearch(int aim, int v[], int n)
{
	int i = 0;
	for(;v[i]<=aim;i++)
	{
		if (v[i] == aim)return (i);
	}
}

int main()
{
	int arr[10000] = { 0 };
	int i;
	int n = 0;
	int x = 0;
	for(i=0; scanf_s("%d",&arr[i])!=0;i++)
	{
		;
	}
	scanf_s("%d", &n);
	scanf_s("%d", &x);
	binsearch(x, arr, n);
}
